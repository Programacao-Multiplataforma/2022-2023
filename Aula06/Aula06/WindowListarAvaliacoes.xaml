<Window x:Class="Aula06.WindowListarAvaliacoes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aula06"
        mc:Ignorable="d"
        Title="Listar Avaliações" Height="181" Width="270">
    <Grid>
        <ListView x:Name="lvClassificacoes">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Classificações" Width="100"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
